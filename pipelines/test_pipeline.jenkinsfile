pipeline {
    agent any

    def runCmd
    stages {
        stage('Example Stage') {
            steps {
                script {
                    // 加载并直接获取 RunCmd 的实例
                    bat 'pwd'
                    runCmd = load 'groovy/RunCmd.groovy'
                    // 使用 runCmd 方法
                    runCmd.runCmd('python --version')
                    load 'pipelines/base/build_python_env.groovy'(runCmd)
                }
            }
        }
    }
}
